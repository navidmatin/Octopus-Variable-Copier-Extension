{
	"manifest_version": 2,

	"name": "Octopus Variable Copier- An unofficial tool for copying variable sets",
	"description": "This extension is used for copying variable sets. More Info:",
	"version": "1.0",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "Click here!"
	},
	"background": {
		"scripts": ["octopusController.js", "octopusServerInfo.js", "content/sjcl/sjcl.js"],
		"persistent": false
	},
	"permissions": [
   "storage",
    "http://*/"
  ]
}